type Link {
  id: ID!
  title: String!
  description: String!
  imageUrl: String!
  url: String!
  createdAt: Time!
  updatedAt: Time!
}

input CreateLinkInput {
  id: ID!
  title: String!
  description: String!
  imageUrl: String!
  url: String!
}

extend type Mutation {
  createLink(createLinkInput: CreateLinkInput!): Link!
}

extend type Query {
  link(id: ID!): Link
}
